string += string
  `$this = _strJoin($this, $arg)` string

string += integer
  `$this = _strJoin($this, _strFromInt($arg))` string

string + string
  `_strJoin($this, $arg)` string

string + integer
  `_strJoin($this, _strFromInt($arg))` string

string + float
  `_strJoin($this, _strFromFloat($arg))` string

string getType
  `"string"` string

string == string
  `(strcmp($this, $arg) == 0)` bool
  block
    `condBool = (strcmp($this, $arg) == 0); if (condBool) {$block}` bool

