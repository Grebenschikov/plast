// superstructs
* : *
  `.$thisName = $arg` struct

// array_integer

variable = array_integer
  `$this = $arg` array_integer array_integer

variable = array_string
  `$this = $arg` array_string array_string

array_integer each variable
  `for(int i = 0; i < kv_size($this); i++) {$arg = kv_a($thisSubType, $this, i); $block}` undefined integer

array_integer push integer
  `kv_push(int, $this, $arg)`

array_integer pop
  `kv_pop($this)` integer

array_integer print
  out = ""
  this each item
    out == ""
      out += item
    else
      out += ", " + item
  out print
